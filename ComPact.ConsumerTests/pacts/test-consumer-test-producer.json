{
  "consumer": {
    "name": "test-consumer"
  },
  "provider": {
    "name": "test-producer"
  },
  "interactions": [
    {
      "description": "a request",
      "request": {
        "method": 2,
        "path": "/testpath",
        "headers": {
          "Accept": "application/json"
        },
        "query": ""
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "number": 42,
          "text": "Hello World!"
        },
        "matchingRules": {
          "$.body.number": {
            "match": "type"
          },
          "$.body.text": {
            "match": "regex",
            "regex": "^Hello.*$"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}